// Add route aliases for /endofday/* endpoints mapping to existing /eod/* handlers
addEventListener('fetch', event => {
    const request = event.request;
    const url = new URL(request.url);

    // Check if the request starts with /endofday/
    if (url.pathname.startsWith('/endofday/')) {
        // Replace /endofday/ with /eod/
        url.pathname = url.pathname.replace('/endofday/', '/eod/');
        // Create a new request object with the updated URL
        const newRequest = new Request(url, request);
        // Forward the request to the existing handler
        event.respondWith(handleRequest(newRequest));
    } else {
        // If not matching, handle normally
        event.respondWith(handleRequest(request));
    }
});

async function handleRequest(request) {
    // Your existing handling logic here
}